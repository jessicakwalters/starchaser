<section class="all-park-card-wrapper ">
    <div>
        <% let day = new Date((data.parkData.locations[0].forecasts[idx].time * 1000)); 
        let weekdays = ['Sunday', 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday']%>
      <h3><%= weekdays[day.getDay()] %></h3>
      <section class="flex-row">
        <div class="flex moonphase-icon <%= data.parkData.locations[0].forecasts[idx].moonPhase %>">
          <h4>Moonphase</h4>
          <%# data.parkData.locations[0].forecasts[idx].moonPhase %>

          <!-- <i class="fas fa-moon fa-2x"></i> -->
        </div>
        <div class="flex weather-icon <%= data.parkData.locations[0].forecasts[idx].icon %>">
          <h4>Weather</h4>          
        </div>
      </section>
        <div class="outlook flex <%= data.parkData.locations[0].forecasts[idx].outlook %>">
          <h5>Outlook</h5>
        </div>      
    </div>
  </section>